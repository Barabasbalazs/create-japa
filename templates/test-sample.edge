import { test } from '@japa/runner'

@if(assertPlugin)~
test.group('Maths.add', () => {
  test('add two numbers', ({ {{ assertPlugin }} }) => {
    // Test logic goes here
    @if (assertPlugin === 'assert')
    assert.equal(1 + 1, 2)
    @else
    expect(1 + 1).toBe(2)
    @endif
  })
})
@else~
test.group('Maths.add', () => {
  test('add two numbers', () => {})
})
@end
